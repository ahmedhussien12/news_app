import 'package:dio/dio.dart';

class NewsService {
  final Dio dio;

  NewsService(this.dio);

  getNews() async {
    Response response = await dio.get(
        'https://newsapi.org/v2/top-headlines?apiKey=87055519e0f64a7d9c2409f9d59d7d2f&country=us&category=general');
    Map<String, dynamic> jsonData = response.data;
  }
}
